<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper
	namespace="com.ssafy.happy.repository.mapper.GwangJinSectorMapper">

	<select id="getSector" resultType="sector">
		SELECT * FROM market m LIMIT
		0,1000
	</select>

	<select id="search" resultType="sector">
              select *
      from market m
      where m.guguncode=#{gugun}
      LIMIT 0,100

	</select>

</mapper>